		<<endif>>\
		<<if $player.owns($items.Waxing)>>\
		<<removeFromInv $items.Waxing.name>>\
		<<endif>>\
	<<endif>>\
\
	<<if (not $player.flags.salonManicure) and ($player.semiPermManicure lte 0) and ($player.permManicure lte 0)>>\
		<<if ($player.owns($items.Manicure))>>\
			<<removeFromInv $items.Manicure.name>>\
		<<endif>>\
		<<if ($player.owns($items.GarishManicure))>>\
			<<removeFromInv $items.GarishManicure.name>>\
		<<endif>>\
	<<endif>>\
\
	<<if not $player.flags.salonMakeup>>\
		<<if $player.owns($items.SubtleMakeup) and ($player.semiPermMakeup lte 0) and ($player.permMakeup lte 0)>>\
			<<removeFromInv $items.SubtleMakeup.name>>\
		<<endif>>\
		<<if $player.owns($items.NormalMakeup) and ($player.semiPermMakeup lte 1) and ($player.permMakeup lte 1)>>\
			<<removeFromInv $items.NormalMakeup.name>>\
		<<endif>>\
		<<if $player.owns($items.BimboMakeup) and ($player.semiPermMakeup lte 2) and ($player.permMakeup lte 2)>>\
			<<removeFromInv $items.BimboMakeup.name>>\
		<<endif>>\
		<<if $player.owns($items.HeavyMakeup) and ($player.semiPermMakeup lte 3) and ($player.permMakeup lte 3)>>\
			<<removeFromInv $items.HeavyMakeup.name>>\
		<<endif>>\
	<<endif>>\
	<<if ($player.scoreMakeup() eq 0) and (($player.semiPermMakeup gt 0) or ($player.permMakeup gt 0))>>\
		<<if ($player.semiPermMakeup eq 1) or ($player.permMakeup eq 1)>>\
			<<addToInv $items.SubtleMakeup.name>>\
		<<elseif ($player.semiPermMakeup eq 2) or ($player.permMakeup eq 2)>>\
			<<addToInv $items.NormalMakeup.name>>\
		<<elseif ($player.semiPermMakeup eq 3) or ($player.permMakeup eq 3)>>\
			<<addToInv $items.BimboMakeup.name>>\
		<<endif>>\
	<<endif>>\
\
	<<if (not $player.flags.salonLips)>>\
		<<if $player.owns($items.LipsEnhancingXL) and ($player.semiPermLips lte 1) and ($player.permLips lte 1)>>\
			<<removeFromInv $items.LipsEnhancingXL.name>>\
			<<addToInv $items.LipsEnhancing.name>>\
		<<elseif $player.owns($items.LipsEnhancing) and ($player.semiPermLips eq 0) and ($player.permLips eq 0)>>\
			<<removeFromInv $items.LipsEnhancing.name>>\
		<<endif>>\
	<<endif>>\
\
	<<if (not $player.flags.salonAss)>>\
		<<if $player.owns($items.AssEnhancingXL) and ($player.semiPermAss lte 1) and ($player.permAss lte 1)>>\
			<<removeFromInv $items.AssEnhancingXL.name>>\
			<<addToInv $items.AssEnhancing.name>>\
		<<elseif $player.owns($items.AssEnhancing) and ($player.semiPermAss lte 0) and ($player.permAss lte 0)>>\
			<<removeFromInv $items.AssEnhancing.name>>\
		<<endif>>\
	<<endif>>\
\
	<<if (not $player.flags.salonBreast)>>\
		<<if $player.owns($items.breastImplantsDD) and ($player.semiPermBoobs lte 3) and ($player.permBoobs lte 3)>>\
			<<removeFromInv $items.breastImplantsDD.name>>\
			<<addToInv $items.breastImplantsC.name>>\
		<<elseif $player.owns($items.breastImplantsC) and ($player.semiPermBoobs lte 2) and ($player.permBoobs lte 2)>>\
			<<removeFromInv $items.breastImplantsC.name>>\
			<<addToInv $items.breastImplantsB.name>>\
		<<elseif $player.owns($items.breastImplantsB) and ($player.semiPermBoobs lte 1) and ($player.permBoobs lte 1)>>\
			<<removeFromInv $items.breastImplantsB.name>>\
			<<addToInv $items.breastImplantsA.name>>\
		<<elseif $player.owns($items.breastImplantsA) and ($player.semiPermBoobs lte 0) and ($player.permBoobs lte 0)>>\
			<<removeFromInv $items.breastImplantsA.name>>\
		<<endif>>\
	<<endif>>\
\
	<<if (not $player.flags.salonAnal)>>\
		<<if $player.owns($items.analSmoothing3) and ($player.semiPermAnal lte 2) and ($player.permAnal lte 2)>>\
			<<removeFromInv $items.analSmoothing3.name>>\
			<<addToInv $items.analSmoothing2.name>>\
