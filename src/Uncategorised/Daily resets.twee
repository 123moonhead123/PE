	<<set $player.flags.salonAss to false>>\
	<<set $player.flags.salonBreast to false>>\
	<<set $player.flags.salonAnal to false>>\
	<<set $player.flags.salonNose to false>>\
\
	<<set $player.flags.choreReward = false>>\
